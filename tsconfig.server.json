{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "module": "esnext",
        "moduleResolution": "bundler",
        "esModuleInterop": true,
        "outDir": "dist",
        "noEmit": false,
        "isolatedModules": false
    },
    "include": [
        "server.ts",
        "services/**/*",
        "models/**/*",
        "constants.tsx",
        "types.ts"
    ]
}